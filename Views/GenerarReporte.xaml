<Window x:Class="Inventario_Hotel.Views.GenerarReporte"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inventario_Hotel.Views"
        mc:Ignorable="d"
        Title="GenerarReporte" Height="450" Width="800">
    <Grid>

        <StackPanel Orientation="Vertical" Margin="19,112,21,65">
            <TextBlock Text="Seleccione Año" FontSize="16"/>
            <ComboBox x:Name="cbAnios" SelectionChanged="cbAnios_SelectionChanged"/>

            <TextBlock Text="Seleccione Mes" FontSize="16" Margin="0,20,0,0"/>
            <ListBox x:Name="lbMeses" />

            <Button Content="Generar Reporte" Click="GenerarReporte_Click" Margin="0,20,0,0"/>
            
        </StackPanel>
        <Button Content="Salir" Click="Salir_Click" Margin="19,360,603,42"/>
        <Image Source="/Views/Logo.jpeg" Margin="321,16,321,372" RenderTransformOrigin="2.19,1.106"/>

    </Grid>
</Window>
